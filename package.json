{
  "name": "zupa_rider",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "apps": "react-native run-android && react-native run-ios --simulator='iPhone 11 Pro'",
    "android": "react-native run-android",
    "iphone8": "react-native run-ios --simulator='iPhone 8'",
    "iphone11 pro": "react-native run-ios --simulator='iPhone 11 Pro'",
    "iphone11 pro max": "react-native run-ios --simulator='iPhone 11 Pro Max'",
    "iphone12 pro": "react-native run-ios --simulator='iPhone 12 Pro'",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "debug": "react-native run-android --variant=debug",
    "release": "react-native run-android --variant=release",
    "test": "jest",
    "lint": "eslint .",
    "build-android": "cd android &&./gradlew clean assembleRelease",
    "bundle-android": "cd android &&./gradlew clean && gradlew bundleRelease",
    "clean-android": "cd android &&./gradlew clean"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.12.4",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "dateformat": "^4.5.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-animatable": "^1.3.3",
    "react-native-animate-loading-button": "^1.0.3",
    "react-native-datepicker": "^1.7.2",
    "react-native-email": "^1.1.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-keyboard-aware-scrollview": "^0.0.7",
    "react-native-open-maps": "^0.3.5",
    "react-native-paper": "^4.7.2",
    "react-native-phone-call": "^1.0.9",
    "react-native-raw-bottom-sheet": "^2.2.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1",
    "react-native-sms": "^1.11.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/runtime": "^7.13.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.1",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./__mocks__/@react-native-async-storage/async-storage.js"
    ]
  }
}
